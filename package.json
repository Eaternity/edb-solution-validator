{
  "name": "edb-solution-validator",
  "version": "1.0.0",
  "description": "Edb-solution validator",
  "main": "index.js",
  "lint-staged": {
    "src/**/*.js": ["npm run build", "eslint --fix", "git add"]
  },
  "jest": {
    "testMatch": ["<rootDir>/src/**/*.test.js"]
  },
  "scripts": {
    "test": "jest src --watch",
    "lint": "node_modules/.bin/eslint ./src",
    "build": "npx babel src/ -d dist/ --source-maps",
    "debug": "npm run build && inspect dist/debug.js",
    "precommit": "lint-staged"
  },
  "keywords": ["edb-solution", "validator"],
  "author": "mcmunder",
  "dependencies": {
    "json2csv": "^3.11.5",
    "jsonfile": "^4.0.0",
    "jsonschema": "^1.2.0",
    "ramda": "^0.25.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "eslint": "^4.9.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "husky": "^0.14.3",
    "inspect-process": "^0.5.0",
    "jest": "^21.2.1",
    "lint-staged": "^4.3.0"
  }
}
